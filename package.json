    "expo-constants": "~12.1.3",
    "expo-firebase-recaptcha": "~2.0.2",
    "expo-status-bar": "~1.1.0",
    "firebase": "^9.2.0",
    "firebase-admin": "^10.0.0",
    "husky": "^7.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.2",
    "react-native-safe-area-context": "^3.3.2",
